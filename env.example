# Telegram Bot Configuration
BOT_TOKEN=your_telegram_bot_token_here
WEBHOOK_URL=https://your-domain.com/webhook
WEBHOOK_PATH=/webhook

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/iqstocker
REDIS_URL=redis://localhost:6379/0

# LLM Providers (add one of the keys)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Encryption key for API keys in DB (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
ENCRYPTION_KEY=your_32_byte_base64_key_here

# Payment Provider (Boosty)
BOOSTY_CLIENT_ID=your_boosty_client_id
BOOSTY_CLIENT_SECRET=your_boosty_client_secret
BOOSTY_WEBHOOK_SECRET=your_boosty_webhook_secret

# Admin Panel
ADMIN_SECRET_KEY=your_secret_key_for_admin_panel
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin_password

# Logging and Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=INFO

# File Storage
UPLOAD_FOLDER=uploads
MAX_FILE_SIZE=10485760  # 10MB

# Rate Limiting
ADOBE_STOCK_RATE_LIMIT=10  # requests per second
REDIS_CACHE_TTL=2592000  # 30 days in seconds

# Proxy Configuration
PROXY_FILE=proxies.txt  # Path to file with proxy list

# Application Settings
DEBUG=False
HOST=0.0.0.0
PORT=8000
